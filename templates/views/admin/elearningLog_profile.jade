extends ../../layouts/admin-default


block content

    .col-sm-9.col-sm-offset-3.col-md-10.col-md-offset-3.main 
        .row
            h3 #{data.elearning_logs._id}
         form.row
            .row
                .col-md-1
                    p User
                .col-md-2
                    mark #{data.elearning_logs.user}
            .row
                .col-md-1
                    p Event
                .col-md-2
                    mark #{data.elearning_logs.event}
            .row
                h4 Meta
            .row
                .col-md-1
                    p CreatedAt
                .col-md-3
                    mark #{data.elearning_logs.createdAt}
            .row
                .col-md-1
                    p UpdatedAt
                .col-md-3
                    mark #{data.elearning_logs.updatedAt}
            button.btn.btn-primary(type='submit') Save
        form.form-group.row(method='post', action='/admin/elearning-logs/'+data.elearning_logs._id)
            input(type='hidden', name='action', value='deleteELearningLog')
            button.btn.btn-link(type='submit', name='deleteELearningLog') Delete ELearning Log
